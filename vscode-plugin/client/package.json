{
  "name": "fulibFeedback-vscode-client",
  "description": "VSCode client for the fulibFeedback language server",
  "author": "Adrian Kunz",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fujaba/fulibFeedback"
  },
  "scripts": {
    "vscode:prepublish": "pnpm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "pnpm run esbuild-base -- --sourcemap",
    "esbuild:watch": "pnpm run esbuild-base -- --sourcemap --watch",
    "build:watch": "tsc --watch"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/vscode": "^1.63.1",
    "@vscode/test-electron": "^2.1.0",
    "esbuild": "^0.14.11",
    "typescript": "^4.5.4"
  }
}
